{
    "name": "@via-profit-services/legal-entity",
    "support": "https://via-profit.ru",
    "version": "1.2.0",
    "description": "Via Profit Legal Entity Schema",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 12.8.3",
        "npm": ">= 6.14.8"
    },
    "keywords": [
        "graphql",
        "legal-entity"
    ],
    "scripts": {
        "postinstall": "node ./scripts/postinstall.js",
        "dist": "yarn lint && yarn build:dist",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "build:migrations": "webpack --config ./webpack/webpack-config-migrations.ts",
        "start": "yarn build:dev --watch",
        "debug": "cross-env DEBUG=true yarn build:dev --watch",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "test": "jest",
        "lint": "yarn dlx eslint --ext .ts ./src/ --fix .",
        "migrate:latest": "yarn knex migrate:latest --knexfile ./.knex/knexfile.js",
        "migrate:rollback": "yarn knex migrate:rollback --knexfile ./.knex/knexfile.js",
        "migrate:rollback:all": "yarn knex migrate:rollback --all --knexfile ./.knex/knexfile.js",
        "migrate:up": "yarn knex migrate:up --knexfile ./.knex/knexfile.js",
        "migrate:down": "yarn knex migrate:down --knexfile ./.knex/knexfile.js",
        "migrate:make": "yarn knex migrate:make --migrations-directory ./src/database/migrations -x ts",
        "seed:run": "yarn knex seed:run --knexfile ./.knex/knexfile.js",
        "seed:run:specific": "yarn knex seed:run --knexfile ./.knex/knexfile.js --specific=",
        "postversion": "git push --tags",
        "release:patch": "yarn version patch",
        "release:minor": "yarn version minor",
        "release:major": "yarn version major",
        "release:prerelease": "yarn version prerelease"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/legal-entity.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vaily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "peerDependencies": {
        "@via-profit-services/core": "^1.0.5",
        "@via-profit-services/geography": "^1.0.0",
        "@via-profit-services/redis": "^1.0.3",
        "ioredis": "^4.27.3",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.28",
        "uuid": "^8.3.1",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    },
    "devDependencies": {
        "@graphql-tools/schema": "^7.1.3",
        "@graphql-tools/utils": "^7.5.2",
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@types/express": "^4.17.11",
        "@types/express-serve-static-core": "^4.17.2",
        "@types/faker": "^5.1.4",
        "@types/fs-extra": "^9.0.4",
        "@types/jest": "^24.0.23",
        "@types/moment-timezone": "^0.5.12",
        "@types/node": "^14.14.10",
        "@types/shelljs": "^0.8.8",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^8.3.0",
        "@types/webpack": "^4.41.2",
        "@types/webpack-bundle-analyzer": "^3.9.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "@via-profit-services/core": "^1.1.9",
        "@via-profit-services/eslint-config": "^0.1.11",
        "@via-profit-services/geography": "^1.2.0",
        "@via-profit-services/knex": "^1.1.4",
        "@via-profit-services/redis": "^1.0.3",
        "@via-profit/dataloader": "^1.0.0",
        "chalk": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "express": "^4.17.1",
        "faker": "^5.1.0",
        "filemanager-webpack-plugin": "^2.0.5",
        "fs-extra": "^9.0.1",
        "graphql": "^15.5.0",
        "graphql-tag": "^2.11.0",
        "husky": "^4.2.3",
        "ioredis": "^4.27.3",
        "jest": "^24.9.0",
        "jest-transform-graphql": "^2.1.0",
        "knex": "^0.95.4",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.33",
        "nodemon": "^2.0.2",
        "nodemon-webpack-plugin": "^4.3.2",
        "pg": "^8.5.1",
        "prettier": "^2.1.2",
        "raw-loader": "^4.0.2",
        "remove-files-webpack-plugin": "^1.2.2",
        "shelljs": "^0.8.4",
        "supertest": "^4.0.2",
        "terser-webpack-plugin": "^2.3.5",
        "ts-jest": "^24.2.0",
        "ts-loader": "^8.0.11",
        "ts-node": "^9.0.0",
        "typescript": "^4.1.2",
        "uuid": "^8.3.2",
        "webpack": "^5.7.0",
        "webpack-bundle-analyzer": "^4.1.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.10.1",
        "webpack-merge": "^5.4.0",
        "webpack-node-externals": "^1.7.2",
        "webpack-shell-plugin": "^0.5.0",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.1"
    }
}
